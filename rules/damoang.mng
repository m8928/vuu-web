    "rules": {
        "default": {
            "articles": {
                "urlPattern": ".*\\/[a-zA-Z0-9]+(\\?.*)?$",
                "repeatable": true,
                "selector": "ul.list-group li.list-group-item:not(.d-none)",
                "fields": {
                    "title": {
                        "selector": ".da-article-link",
                        "type": "string",
                        "normalization": [
                            {
                                "type": "trim"
                            }
                        ]
                    },
                    "link": {
                        "selector": ".da-article-link",
                        "attribute": "href",
                        "type": "string"
                    },
                    "id": {
                        "selector": ".da-article-link",
                        "attribute": "href",
                        "type": "int",
                        "normalization": [
                            {
                                "type": "regexReplace",
                                "pattern": ".*\\/(\\d+).*",
                                "replacement": "$1"
                            }
                        ]
                    },
                    "author": {
                        "selector": ".sv_member",
                        "type": "string",
                        "normalization": [
                            {
                                "type": "trim"
                            }
                        ]
                    },
                    "date": {
                        "selector": ".wr-date",
                        "type": "string",
                        "normalization": [
                            {
                                "type": "regexReplace",
                                "pattern": "등록",
                                "replacement": ""
                            },
                            {
                                "type": "trim"
                            }
                        ]
                    },
                    "view": {
                        "selector": ".wr-num.order-4",
                        "type": "int",
                        "normalization": [
                            {
                                "type": "regexReplace",
                                "pattern": "조회",
                                "replacement": ""
                            },
                            {
                                "type": "trim"
                            },
                            {
                                "type": "convertKNotation"
                            }
                        ]
                    },
                    "reply": {
                        "selector": ".count-plus",
                        "type": "int",
                        "default": "0"
                    },
                    "thumbUp": {
                        "selector": ".rcmd-box",
                        "type": "int",
                        "default": "0"
                    },
                    "articleType": {
                        "selector": ".rcmd-box",
                        "type": "string",
                        "default": "",
                        "normalization": [
                            {
                                "type": "trim"
                            }
                        ],
                        "condition": {
                            "equals": "홍보"
                        }
                    }
                }
            },
            "content": {
                "urlPattern": ".*\\/[a-zA-Z0-9]+\\/\\d+.*",
                "repeatable": false,
                "optional": true,
                "selector": "#bo_v",
                "fields": {
                    "title": {
                        "selector": "#bo_v_title",
                        "type": "string",
                        "normalization": [
                            {
                                "type": "trim"
                            }
                        ]
                    },
                    "author": {
                        "selector": ".sv_member",
                        "type": "string",
                        "normalization": [
                            {
                                "type": "trim"
                            }
                        ]
                    },
                    "date": {
                        "selector": "#bo_v_info .line-top > div:last-child",
                        "type": "string",
                        "normalization": [
                            {
                                "type": "trim"
                            }
                        ]
                    },
                    "body": {
                        "selector": "#bo_v_con .na-convert, #bo_v_data .text-truncate",
                        "type": "html",
                        "joinValues": true
                    }
                }
            },
            "comments": {
